#include <stdio.h>
#include <iostream>
using namespace std;

void update(int *a,int *b) {
    int sum ;
    sum = *a + *b;
    
    
    int subtraction;
    subtraction = abs(*a - *b);
    
    *a =sum ;
    *b = subtraction;
    
}

int main() {
    int a, b;
    cin >> a >> b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);
    cout << a << endl << b;

    return 0;
}
